{
  "version": 2,
  "builds": [
    {
      "src": "Food-Bridge/Food-bridge/server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "Food-Bridge/Food-bridge/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Food-Bridge/Food-bridge/server/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/Food-Bridge/Food-bridge/$1"
    }
  ]
}